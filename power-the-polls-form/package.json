{
   "name": "@ptp-us/power-the-polls-form",
   "version": "0.9.2",
   "description": "Power the Polls Volunteer Form",
   "main": "dist/index.js",
   "module": "dist/index.mjs",
   "es2015": "dist/esm/index.mjs",
   "es2017": "dist/esm/index.mjs",
   "types": "dist/types/index.d.ts",
   "collection": "dist/collection/collection-manifest.json",
   "collection:main": "dist/collection/index.js",
   "unpkg": "dist/power-the-polls-form/power-the-polls-form.js",
   "scripts": {
      "dev": "stencil build --dev --watch --serve",
      "dev:site": "npm run clean && stencil build --watch",
      "build": "stencil build --docs --no-cache",
      "build:copy": "copyfiles -u 1 ./dist/**/* ../dist/power-the-polls-form/ && copyfiles package.json ../dist/power-the-polls-form/ && copyfiles Readme.md ../dist/power-the-polls-form/",
      "release": "npm run clean-all && npm run build && npm run build:copy && replace \"dist/\" \"\" ../dist/power-the-polls-form/package.json",
      "start": "npm run dev",
      "clean": "rimraf ./dist",
      "clean-all": "rimraf ./www && rimraf ../dist/power-the-polls-form/ && npm run clean",
      "lint": "tslint --project .",
      "lint:fix": "tslint --fix --force --project .",
      "test": "stencil test --spec --e2e",
      "test.watch": "stencil test --spec --e2e --watchAll",
      "generate": "stencil generate"
   },
   "dependencies": {
      "@stencil/core": "latest",
      "@stencil/router": "latest"
   },
   "devDependencies": {
      "@rollup/plugin-replace": "^2.3.3",
      "@stencil/core": "^1.17.2",
      "@stencil/react-output-target": "0.0.7",
      "@stencil/router": "^1.0.1",
      "@stencil/sass": "^1.3.2",
      "copyfiles": "^2.3.0",
      "cross-env": "^7.0.2",
      "replace": "^1.2.0",
      "rimraf": "^3.0.2",
      "rollup-plugin-dotenv": "^0.3.0",
      "rollup-plugin-terser": "^7.0.0",
      "tslint": "^6.1.2",
      "typescript": "^3.9.7"
   },
   "license": "MIT"
}
