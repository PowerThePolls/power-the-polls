{
   "name": "@ptp-us/power-the-polls-form",
   "version": "1.1.79",
   "description": "Power the Polls Volunteer Form",
   "license": "MIT",
   "homepage": "https://powerthepolls.org",
   "repository": {
      "type": "git",
      "url": "https://github.com/PowerThePolls/power-the-polls"
   },
   "main": "dist/index.cjs.js",
   "module": "dist/full/index.js",
   "es2015": "dist/esm/index.js",
   "es2017": "dist/esm/index.js",
   "types": "dist/types/index.d.ts",
   "collection": "dist/collection/collection-manifest.json",
   "collection:main": "dist/collection/index.js",
   "unpkg": "dist/power-the-polls-form/power-the-polls-form.js",
   "scripts": {
      "dev": "stencil build --dev --watch --serve",
      "dev:site": "npm run clean && stencil build --watch",
      "build": "npm run lint && stencil build --prod --docs --no-cache && terser ./dist/full/index.js -o ./dist/full/index.js",
      "build:clean": "npm run clean-all && npm run build",
      "package": "copyfiles -E -u 1 \"./dist/**/*\" \"../dist/power-the-polls-form/\" && copyfiles -E package.json Readme.md LICENSE ../dist/power-the-polls-form/ && replace \"dist/\" \"\" ../dist/power-the-polls-form/package.json && cross-env-shell replace \"CURRENT_VERSION\" \"$npm_package_version\" ../dist/power-the-polls-form/ -r --include=\"*.js\"",
      "deploy": "cd ../dist/power-the-polls-form && npm publish && cd ../power-the-polls-form",
      "deploy:beta": "cd ../dist/power-the-polls-form && npm publish --tag beta && cd ../power-the-polls-form",
      "release": "npm whoami && npm run test && npm run clean-all && npm run build && npm run package && npm run deploy",
      "release:beta": "npm whoami && npm run test && npm run clean-all && npm run build && npm run package && npm run deploy:beta",
      "clean": "rimraf ./dist",
      "clean-all": "rimraf ./www && rimraf ../dist/power-the-polls-form/ && npm run clean",
      "lint": "tslint --project .",
      "lint:fix": "tslint --fix --force --project .",
      "test": "stencil test --spec --e2e",
      "test:watch": "stencil test --spec --e2e --watchAll",
      "generate": "stencil generate",
      "util:check-updates": "npm-check-updates",
      "util:perform-updates": "npm-check-updates -u"
   },
   "dependencies": {
      "@stencil/core": "~3.4.0",
      "@stencil/router": "^1.0.1",
      "@types/geojson": "^7946.0.10"
   },
   "devDependencies": {
      "@rollup/plugin-replace": "^5.0.2",
      "@stencil/react-output-target": "0.5.3",
      "@stencil/router": "^1.0.1",
      "@stencil/sass": "^3.0.4",
      "@types/jest": "^27.0.3",
      "@types/puppeteer": "5.4.7",
      "copyfiles": "^2.4.1",
      "cross-env": "^7.0.3",
      "dotenv": "^16.3.1",
      "jest": "^29.7.0",
      "jest-cli": "^27.4.5",
      "npm-check-updates": "^16.10.12",
      "puppeteer": "^20.7.3",
      "replace": "^1.2.2",
      "rimraf": "^5.0.1",
      "terser": "^5.18.1",
      "ts-jest": "^27.0.3",
      "tslint": "^6.1.3",
      "typescript": "^5.1.3"
   }
}
