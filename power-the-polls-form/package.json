{
   "name": "ptp-form",
   "version": "1.0.0",
   "description": "Power the Polls Volunteer Form",
   "main": "dist/index.js",
   "module": "dist/index.mjs",
   "es2015": "dist/esm/index.mjs",
   "es2017": "dist/esm/index.mjs",
   "types": "dist/types/index.d.ts",
   "collection": "dist/collection/collection-manifest.json",
   "collection:main": "dist/collection/index.js",
   "unpkg": "dist/ptp-form/ptp-form.js",
   "files": [
      "dist/"
   ],
   "scripts": {
      "dev": "stencil build --dev --watch --serve",
      "build": "stencil build --docs --no-cache",
      "start": "npm run dev",
      "lint": "tslint --project .",
      "lint:fix": "tslint --fix --force --project .",
      "test": "stencil test --spec --e2e",
      "test.watch": "stencil test --spec --e2e --watchAll",
      "generate": "stencil generate"
   },
   "dependencies": {
      "@stencil/core": "latest",
      "@stencil/router": "latest"
   },
   "devDependencies": {
      "@stencil/core": "^1.17.2",
      "@stencil/react-output-target": "0.0.7",
      "@stencil/router": "^1.0.1",
      "@stencil/sass": "^1.3.2",
      "cross-env": "^7.0.2",
      "tslint": "^6.1.2",
      "typescript": "^3.9.7"
   },
   "license": "MIT"
}
