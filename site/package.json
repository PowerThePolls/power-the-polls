{
   "name": "power-the-polls-site",
   "private": false,
   "version": "2.0.0",
   "description": "Power the Polls",
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/pizza-to-the-polls/power-the-polls"
   },
   "scripts": {
      "dev": "stencil build --dev --watch --serve",
      "dev:form": "cd ../power-the-polls-form && npm run build && cd ../site && npm run dev",
      "build": "npm run lint && stencil build --docs --no-cache",
      "release": "npm run build",
      "start": "npm run dev",
      "lint": "tslint --project .",
      "lint:fix": "tslint --fix --force --project .",
      "generate": "stencil generate"
   },
   "devDependencies": {
      "@rollup/plugin-replace": "^2.3.3",
      "@stencil/core": "^1.17.2",
      "@stencil/router": "^1.0.1",
      "@stencil/sass": "^1.3.2",
      "@types/node": "^14.0.27",
      "cross-env": "^7.0.2",
      "dotenv": "^8.2.0",
      "tslint": "^6.1.2",
      "typescript": "^3.9.7",
      "workbox-build": "^4.3.1"
   },
   "dependencies": {
      "@ptp-us/power-the-polls-form": "^0.11.4"
   }
}
